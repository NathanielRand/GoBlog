{{define "yield"}}
<div class="container">
    <div class="row">
        <div class="col s12 m12">
            <div class="card-content">
                <div class="card-title">
                    <h4>
                        <img src="data:image/svg+xml;utf8;base64," />
                         COLLECTION</h4>
                </div>
                <div class="divider"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <nav>
            <div class="nav-wrapper">
                <form>
                    <div class="input-field">
                        <input id="search" type="search" class="white blue-grey-text light" required>
                        <label class="label-icon" for="search">
                            <img src="data:image/svg+xml;utf8;base64," />
                        </label>
                        <i class="material-icons">close</i>
                    </div>
                </form>
            </div>
        </nav>
        <div id="collection-menu-icons" class="nav-content">
            <ul class="tabs tabs-transparent">
                <li class="tab">
                    <a href="#collection-modification-tab" class="active blue-grey-text">
                        <img src="data:image/svg+xml;utf8;base64," />
                    </a>
                </li>
                <li class="tab">
                    <a href="#collection-micropost-tab" class="blue-grey-text">
                        <img src="data:image/svg+xml;utf8;base64," />
                    </a>
                </li>
                <li class="tab">
                    <a href="#collection-member-tab" class="blue-grey-text">
                        <img src="data:image/svg+xml;utf8;base64," />
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="row">
        {{template "collection-modification-content"}}
        {{template "collection-micropost-content"}}
        {{template "collection-member-content"}}
    </div>
</div>
{{end}}

{{define "collection-modification-content"}}
    <div id="collection-modification-tab" class="col s12"><br>
        <div class="center">
            <!-- Collection Filter Trigger -->
            <a class="waves-effect waves-light btn modal-trigger blue-grey lighten-3 left" href="#modal1"><i class="material-icons">filter_list</i></a>
            
            <!-- Collection Filter Modal Structure -->
            <div id="modal1" class="modal bottom-sheet">
                <div class="modal-content">
                    <h5>Tags</h5>
                    <div class="chips chips-placeholder"></div>
                    <h5>Rating</h5>
                    <form action="#">
                        <p class="range-field">
                            <input type="range" id="collection-modification-filter-slider" min="1" max="5" />
                        </p>
                    </form>
                </div>
                <div class="modal-footer">
                    <a href="#!" class="modal-action modal-close waves-effect waves-green red lighten-3 btn">SEARCH</a>
                </div>
            </div>
            
            <!--Collection Contribute Button-->
            <a href="/modifications/new" class="btn waves-effect waves-light red lighten-3 right"><i class="material-icons">add</i></a>
        </div>
        <br><br>
        
            <ul class="collection">
                <li class="collection-item avatar">
                    <img src="assets/media/nathaniel.png" alt="" class="circle">
                    <span class="title">Title</span>
                    <p>First Line <br>
                    Second Line
                    </p>
                    <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
                </li>
                <li class="collection-item avatar">
                    <img src="assets/media/nathaniel.png" alt="" class="circle">
                    <span class="title">Title</span>
                    <p>First Line <br>
                    Second Line
                    </p>
                    <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
                </li>
                <li class="collection-item avatar">
                    <img src="assets/media/nathaniel.png" alt="" class="circle">
                    <span class="title">Title</span>
                    <p>First Line <br>
                    Second Line
                    </p>
                    <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
                </li>
                <li class="collection-item avatar">
                    <img src="assets/media/nathaniel.png" alt="" class="circle">
                    <span class="title">Title</span>
                    <p>First Line <br>
                    Second Line
                    </p>
                    <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
                </li>
            </ul>
            
    </div>
{{end}}

