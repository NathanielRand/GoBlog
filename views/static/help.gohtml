{{define "yield"}}
    <div class="container">
        {{template "helpHeader"}}
        {{template "helpContent"}}
    </div>
{{end}}

{{define "helpHeader"}}
    <div class="row center">
        <h2>Help</h2>
    </div>
{{end}}

{{define "helpContent"}}
    <div class="card">
        <div class="card-content">
            <div class="card-title">
                <h4>Information</h4>
            </div>    
        </div>
    </div>
{{end}}