{{define "yield"}}
    <div class="container">
        {{template "aboutHeader"}}
        {{template "aboutContent"}}
    </div>
{{end}}

{{define "aboutHeader"}}
    <div class="row center">
        <h2>About</h2>
    </div>
{{end}}

{{define "aboutContent"}}
    <div class="card">
        <div class="card-content">
            <div class="card-title">
                <h4>Information</h4>
            </div>    
        </div>
    </div>
{{end}}