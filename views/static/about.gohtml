{{define "yield"}}
		{{template "aboutHeader"}}
		{{template "aboutMission"}}
		{{template "aboutTeam"}}
{{end}}

{{define "aboutHeader"}}
	<div class="row center">
			<h2>ABOUT</h2>
	</div>
	<div class="row">
		<div class="col s12">
			<div class="card-content">
				<div class="card-title center">
					<img src="data:image/svg+xml;utf8;base64," />
					<h4>Coverd Assurance, Inc.</h4>
					<h5>Medical Pooling</h5><br>
					<p>
						Established 2018<br>
						Born in Charleston, South Carolina
					</p>
				</div>    
			</div>
		</div>
	</div><br>
{{end}}

{{define "aboutMission"}}
	<div class="row">
		<div class="divider"></div>
		<h3 class="center">MISSION</h3>
		<div class="card col s12 m6 l6 offset-m3 offset-l3 z-depth-2">
			<div class="card-content">
				<p><b>Our Mission</b> at Coverd is make health-care affordable for all. Healthcare should be welcomed and not feared. It is our human right to live in peace and in health, free from an all consuming debt.</p>
			</div>
		</div>
	</div>
{{end}}

{{define "aboutTeam"}}
	<div class="row">
		<div class="divider"></div>
		<h3 class="center">TEAM</h3>
	</div>
	<div class="row">
		<div class="col s12 m6 l4">
			<div class="card">
				<div class="card-content center">
					<span>
						<img src="assets/media/nathanielrand.JPG" alt="Nathaniel Rand Image" id="about-image" class="circle responsive-img">
						<h4 class="indigo-text text-lighten-1"><b>Nathaniel</b> Rand</h4>
						<h5 class="orange-text text-lighten-1">Co-Founder</h5><br>
						<h6>TECHNOLOGY</h6>
					</span>
				</div>
			</div>
		</div>
		<div class="col s12 m6 l4">
			<div class="card">
				<div class="card-content center">
					<span>
						<img src="assets/media/nathaniel.png" alt="Jordan Mason Image" id="about-image" class="circle responsive-img">
						<h4 class="indigo-text text-lighten-1"><b>Jordan</b> Mason</h4>
						<h5 class="orange-text text-lighten-1">Co-Founder</h5><br>
						<h6>OPERATIONS</h6>
					</span>
				</div>
			</div>
		</div>
	</div>
{{end}}