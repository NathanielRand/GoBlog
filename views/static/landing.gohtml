{{define "yield"}}
    <div class="container">
        <div class="row">
            <a role="link" href="/enter"><img class="responsive-img col s8 m6 l4 offset-s2 offset-m3 offset-l4" src="/assets/media/nathaniel.png"></a>
        </div>
        <div class="row">
            <div class="center">
                <h2 class="thin blue-grey-text text-lighten-2"><b class="red-text text-lighten-3">GO</b>Blog</h2><br>
                <h4 class="thin blue-grey-text">A Platform for everyone.</h4>
                <h5 class="thin blue-grey-text">Example Photo, Video, Micropost & Broadcast Blog</h5>
                <h6>Built by Nathaniel Rand 2018 Â© All rights reserved</h6>
            </div>
        </div><br>
        <div class="divider"></div>
        <br>
        {{if .Account}}
            <div class="row">
                <div class="col s12 m8 offset-m2">
                    <div class="card-title">
                        <h3>Welcome!</h3>
                        <h5>Account Resources</h5>
                    </div><br>
                    <div class="card-action">
                        <a role="link" href="/galleries" class="col s5 m5 waves-effect waves-light btn red lighten-3 thin z-depth-3">
                            IMAGES</a>
                        <a role="link" href="/microposts" class="col s5 m5 offset-s2 offset-m2 waves-effect waves-light btn red lighten-3 thin z-depth-3">
                            MICROPOSTS</a><br><br><br>
                        <a role="link" href="/videos" class="col s5 m5 waves-effect waves-light btn red lighten-3 thin z-depth-3 disabled">
                            VIDEOS</a>
                        <a role="link" href="/broadcasts" class="col s5 m5 offset-s2 offset-m2 waves-effect waves-light btn red lighten-3 thin z-depth-3 disabled">
                            BROADCAST</a>
                    </div>
                </div>
            </div>
        {{else}}
            <br>
            <div class="row">
                <div class="col s12 m8 offset-m2">
                    <div class="card-action">
                        <a role="link" href="/enter" id="landing-btn-default" class="col s5 l4 waves-effect waves-light btn red lighten-3 thin z-depth-3">
                            <i class="material-icons left">lock_outline</i>LOGIN</a>
                        <a role="link" href="/register" id="landing-btn-default" class=" col s5 l4 offset-s1 waves-effect waves-light btn right thin z-depth-3 blue-grey lighten-2">
                            <i class="material-icons right">person_add</i>JOIN</a>
                    </div>
                </div>
            </div>
        {{end}}
        <br>
        <div class="divider"></div><br>
        <div class="row">
            <div class="col s12 m8 offset-m2">
                <div class="card">
                    <div class="card-content center">
                        <a role="link" href="/images">
                            <img src="data:image/svg+xml;utf8;base64," />
                        </a>
                        <a role="link" href="/images" id="landing-secondary-content"><h4 class="light blue-grey-text">IMAGES</h4></a>
                        <h5>A private image collection</h5>
                        <p class="condensed light blue-grey-text text-darken-1">You can create multiple galleries with unlimited images. Upload and delete images as you please! You can also add a title and description to each gallery.</p>
                    </div>
                    <div class="card-action">
                        <a href="/galleries/new" class="btn waves-effect waves-light red lighten-3" type="submit" name="action">UPLOAD
                            <i class="material-icons right">file_upload</i>
                        </a>
                        <a href="/galleries" class="btn waves-effect waves-light btn-flat blue-grey-text right" type="submit" name="action">VIEW
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m8 offset-m2">
                <div class="card">
                    <div class="card-content center">
                        <a role="link" href="/microposts">
                            <img src="data:image/svg+xml;utf8;base64," />
                        </a>
                        <a role="link" href="/microposts" id="landing-secondary-content"><h4 class="light blue-grey-text">MICROPOSTS</h4></a>
                        <p class="condensed light blue-grey-text text-darken-1">Speak your mind in 66 characters or less. A message that you can also edit.</p>
                    </div>
                    <div class="card-action">
                        <button class="btn waves-effect waves-light red lighten-3" type="submit" name="action">POST
                            <i class="material-icons right">send</i>
                        </button>
                        <button class="btn waves-effect waves-light btn-flat blue-grey-text right" type="submit" name="action">VIEW</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m8 offset-m2">
                <div class="card">
                    <div class="card-content center">
                        <a role="link" href="/videos">
                            <img src="data:image/svg+xml;utf8;base64," />
                        </a>
                        <a role="link" id="landing-secondary-content" href="/video"><h4 class="light blue-grey-text">VIDEOS</h4></a>
                        <p class="condensed light blue-grey-text text-darken-1">Coming Soon!</p>
                    </div>
                    <div class="card-action">
                        <button class="btn waves-effect waves-light red lighten-3 disabled" type="submit" name="action">UPLOAD
                            <i class="material-icons right">file_upload</i>
                        </button>
                        <button class="btn waves-effect waves-light btn-flat blue-grey-text right" type="submit" name="action">LEARN</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m8 offset-m2">
                <div class="card">
                    <div class="card-content center">
                        <a role="link" href="/broadcasts">
                            <img src="data:image/svg+xml;utf8;base64," />
                        </a>
                        <a role="link" id="landing-secondary-content" href="/video"><h4 class="light blue-grey-text">BROADCAST</h4></a>
                        <p class="condensed light blue-grey-text text-darken-1">Coming Soon!</p>
                    </div>
                    <div class="card-action">
                        <button class="btn waves-effect waves-light red lighten-3 disabled" type="submit" name="action">LIVE
                            <i class="material-icons right">settings_input_antenna</i>
                        </button>
                        <button class="btn waves-effect waves-light btn-flat blue-grey-text right" type="submit" name="action">LEARN</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{end}}