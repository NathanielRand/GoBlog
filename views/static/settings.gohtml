{{define "yield"}}
	<div class="row">
        <div class="col s12 m8 offset-m2">
            <div class="card-title center"><br>
                <img src="data:image/svg+xml;utf8;base64," />
                <h3 class="indigo-text text-lighten-2">SETTINGS</h3>
            </div>
        </div>
    </div>
	 <div class="row">
			<div class="col s12 m8 l6 offset-m2 offset-l3">
				<ul class="collapsible popout" data-collapsible="accordion">
					 <li>
						<div class="collapsible-header"><i class="material-icons indigo-text text-lighten-2">lock_outline</i><h6>CREDENTIALS</h6></div>
						<div class="collapsible-body">
							<span><h5 class=" indigo-text text-lighten-2">CREDENTIALS</h5></span>
							<div class='divider'></div><br>
							<div class="center">
								 <i class="material-icons medium indigo-text text-lighten-2">lock_outline</i>
							</div>
							<div class="row center">
								 <form class="col s12">
										<div class="row">
											<div class="input-field col s12">
												 <input id="email" type="email" class="validate">
												 <label for="email">Email</label>
											</div>
										</div>
										<div class="row">
											<div class="input-field col s12">
												 <input id="password" type="password" class="validate">
												 <label for="password">Password</label>
											</div>
										</div>
										<button class="btn waves-effect waves-light z-depth-3" type="submit" name="action">SAVE
											<i class="material-icons right">save</i>
										</button>
								 </form>
							</div>
						</div>
					 </li>
					 <li>
						<div class="collapsible-header"><i class="material-icons indigo-text text-lighten-2">person_outline</i><h6>MEMBERSHIP</h6></div>
						<div class="collapsible-body">
							<span><h5>MEMBERSHIP</h5></span>
							<div class="divider"></div><br>
							<div class="center">
								 <i class="material-icons medium  indigo-text text-lighten-2">person_outline</i>
							</div><br>
							<div class="row">
								 <form action="#">
										<div class="col s3 offset-s1 pull-s1">
											<i class="material-icons large grey-text text-darken-1">donut_large</i>
										</div>
										<h6>BASIC</h6>
										<p>FREE</p>
										<p>
										 <input type="checkbox" id="membership-basic" />
										 <label for="membership-basic">BASIC</label>
										</p><br>
										<div class="divider"></div><br>
										<div class="col s3 offset-s1 pull-s1">
											<i class="material-icons large yellow-text text-darken-2">donut_small</i>
										</div>
										<h6>ELITE</h6>
										<p>$12/annual</p>
										<p>
										 <input type="checkbox" id="membership-elite" />
										 <label for="membership-elite">ELITE</label>
										</p>
								 </form>
							</div>
						</div>
					 </li>
					 <li>
						<div class="collapsible-header"><i class="material-icons indigo-text text-lighten-2">security</i><h6>SECURITY</h6></div>
						<div class="collapsible-body">
							<span><h5>SECURITY</h5></span>
							<div class='divider'></div><br>
							<div class="center">
								 <i class="material-icons medium indigo-text text-lighten-2">security</i>
							</div><br>
							<span>Security Question, 2-Step Authentication (sms, authenticator, email)</span>
						</div>
					 </li>
					 <li>
						<div class="collapsible-header"><i class="material-icons indigo-text text-lighten-2">notifications</i><h6>NOTIFICATIONS</h6></div>
						<div class="collapsible-body">
							<span><h5>NOTIFICATIONS</h5></span>
							<div class='divider'></div><br>
							<div class="center">
								 <i class="material-icons medium indigo-text text-lighten-2">notifications</i>
							</div><br>
							<h6>ACTIVITY</h6><br>
							 <div class="row">
								 <form action="#">
										<br>
										<div class="col s3 offset-s1 pull-s1">
											<i class="material-icons medium indigo-text text-lighten-2">mail_outline</i>
										</div>
										<h6>REQUESTS</h6>
										<p>
										 <input type="checkbox" id="settings-activity-requests" />
										 <label for="settings-activity-requests"></label>
										</p><br>
										<div class="divider"></div><br>
										<br>
										<div class="col s3 offset-s1 pull-s1">
											<i class="material-icons medium indigo-text text-lighten-2">people_outline</i>
										</div>
										<h6>CONNECTIONS</h6>
										<p>
										 <input type="checkbox" id="settings-activity-connections" />
										 <label for="settings-activity-connections"></label>
										</p><br>
										<div class="divider"></div><br>
										<br>
										<div class="col s3 offset-s1 pull-s1">
											<i class="material-icons medium grey-text text-darken-1">sentiment_neutral</i>
										</div>
										<h6>RATINGS</h6>
										<p>
										 <input type="checkbox" id="settings-activity-ratings" />
										 <label for="settings-activity-ratings"></label>
										</p><br>
										<div class="divider"></div><br>
										<br>
										<div class="col s3 offset-s1 pull-s1">
											<i class="material-icons medium grey-text text-darken-1">comment</i>
										</div>
										<h6>COMMENTS</h6>
										<p>
										 <input type="checkbox" id="settings-activity-comments" />
										 <label for="settings-activity-comments"></label>
										</p><br>
										<div class="divider"></div><br>
								 </form>
							</div><br>
							<h6>DELIVERY METHOD</h6><br>
							<div class="switch">
								 <label>
										<i class="material-icons grey-text text-darken-2 small">mail_outline</i> EMAIL
									<input type="checkbox">
									<span class="lever right"></span>
								 </label>
							</div><br>
							<div class='divider'></div><br>
							<div class="switch">
								 <label>
									<i class="material-icons grey-text text-darken-2 small">smartphone</i> PUSH
									<input type="checkbox">
									<span class="lever right"></span>
								 </label>
							</div><br>
							<div class='divider'></div><br>
							<div class="switch">
								 <label>
									 <i class="material-icons grey-text text-darken-2 small">sms</i> SMS/TEXT
									<input type="checkbox">
									<span class="lever right"></span>
								 </label>
							</div><br>
						</div>
					 </li>
					 <li>
						<div class="collapsible-header"><i class="material-icons indigo-text text-lighten-2">credit_card</i><h6>BILLING</h6></div>
						<div class="collapsible-body">
							<span><h5>BILLING</h5></span>
							<div class='divider'></div><br>
							<div class="center">
								 <i class="material-icons medium indigo-text text-lighten-2">credit_card</i>
							</div><br>
							<div class="row">
								 <form class="col s12">
										<div class="row">
										 <div class="input-field col s12 m8 l6 offset-m2 offset-l3">
											<input id="cardnumber" type="text" class="validate">
											<label for="cardnumber">Card Number</label>
										 </div>
										 <div class="input-field col s12 m8 l6 offset-m2 offset-l3">
											<input id="cardexp" type="text" class="validate">
											<label for="cardexp">Card Expiration</label>
										 </div>
											<div class="input-field col s8 m6 l4 offset-m2 offset-l3">
												<input id="zipcdoe" type="text" class="validate">
												<label for="zipcode">Zip Code</label>
											</div>
										</div>
										<div class="row center">
											<button class="btn waves-effect waves-light z-depth-3" type="submit" name="action">SAVE
												<i class="material-icons right">save</i>
											</button>
										</div>
								 </form>
							</div>
						</div>
					 </li>
					 <li>
							<div class="collapsible-header"><i class="material-icons indigo-text text-lighten-2">visibility_off</i><h6>PRIVACY</h6></div>
							<div class="collapsible-body">
								<span><h5>PRIVACY</h5></span>
								<div class='divider'></div><br>
								<div class="center">
									 <i class="material-icons medium indigo-text text-lighten-2">visibility_off</i>
								</div><br>
								<div class="row">

								</div>
							</div>
					 </li>
				</ul>
			</div>
	 </div>
{{end}}