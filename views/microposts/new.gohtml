{{define "yield"}}
    <div class="container">
        <div class="row center">
            <br>
            <img src="data:image/svg+xml;utf8;base64," />
            <h4 class="blue-grey-text text-lighten-1">MICROPOSTS</h4>  
        </div>
        <div class="row">
            {{template "micropostForm"}}
        </div>
    </div>
{{end}}

{{define "micropostForm"}}
    <div class="col s12 m8 offset-m2 card hoverable z-depth-1">
        <div class="card-content">
            <div class="card-title">
                <h6>What is your micropost?</h6>
            </div><br>
            <form action="/microposts" method="POST">
                {{csrfField}}
                <div class="row">
                    <div class="input-field">
                        <input id="micropost-content" type="text" class="validate" name="content" data-length="66">
                        <label for="micropost-content" data-error="Too long" data-success="Accepted">Content</label>
                    </div>
                </div>
                <div class="center"><br>
                    <button type="submit" class="btn waves-effect waves-light red lighten-3">
                        <i class="material-icons">keyboard_arrow_right</i>
                    </button>
                </div>
            </form>
        </div>
    </div>
{{end}}
