{{define "yield"}}
    {{template "healthNavigation"}}
    {{template "healthContent" .}}
{{end}}

{{define "healthNavigation"}}
    <div class="row">
        <div class="col s12 m8 offset-m2">
            <div class="card-title center"><br><br>
                <img src="data:image/svg+xml;utf8;base64," />
                <h3 class="indigo-text text-lighten-2">HEALTH</h3>
            </div>
        </div>
    </div>
{{end}}

{{define "healthContent"}}
    <div class="row">
        <div class="col s12 m8 offset-m2">
            <ul class="tabs tabs-fixed-width">
                <li class="tab col s3"><a href="#customer-dashboard-health" class="active">Status</a></li>
                <li class="tab col s3"><a href="#customer-dashboard-network">Visits</a></li>
            </ul>
        </div>
        {{template "healthStatus" .}}
        {{template "healthVisits" .}}
    </div>
{{end}}

{{define "healthStatus"}}
    <div id="customer-dashboard-health" class="col s12 m12">
        <div class="row">
            <div class="center"><br>
                <h4 class="indigo-text text-lighten-2">STATUS</h4>
                <p>CUSTOMER: #0001
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m8 l5 offset-m2 offset-l1">
                <div class="card">
                    <div class="card-content indigo lighten-2 white-text">
                        <span class="card-title">VISITS</span>
                        <p>Active</p>
                    </div>
                    <div class="card-content">
                        <section>
                            <p><b>Total Visits:</b> <b class="right">{{len .}}</b></p>
                        </section><br>
                        <div class="divider"></div><br>
                        <section>
                            <p>Upcoming Visits: <b class="right">None</b></p><br>
                        </section>
                    </div>
                    <div class="card-action center">
                        <a href="/galleries/new" class="waves-effect waves-light btn amber darken-2"><i class="material-icons left">file_upload</i>NEW VISIT</a>
                    </div>
                </div>
            </div>
            <div class="col s12 m8 l5 offset-m2">
                <div class="card">
                    <div class="card-content indigo lighten-2 white-text">
                        <span class="card-title">CONDITIONS</span>
                        <p>Active & Previous</p>
                    </div>
                    <div class="card-content">
                        <div class="card-title">
                            <h6><b>Active</b></h6>
                            <p>High-Cholesterol, Diabetes</p><br>
                        </div>
                        <div class="divider"></div><br>
                        <div class="card-title">
                            <h6>Previous</h6>
                            <p>High-Cholesterol, Diabetes</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col s12 m8 l5 offset-m2 offset-l1">
                <div class="card">
                    <div class="card-content indigo lighten-2 white-text">
                        <span class="card-title">Medications</span>
                        <p>Active & Previous</p>
                    </div>
                    <div class="card-content">
                        <div class="card-title">
                            <h6><b>Active</b></h6>
                            <p>Lipitor</p><br>
                        </div>
                        <div class="divider"></div><br>
                        <div class="card-title">
                            <h6>Previous</h6>
                            <p>Adderall</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{end}}

{{define "healthVisits"}}
    <div id="customer-dashboard-network">
        <div class="row col s12">
            <div class="center"><br>
                <h4 class="indigo-text text-lighten-2">VISITS</h4>
                <p>CUSTOMER: #00001</p>
            </div>
        </div>
        <div class="row col s12 center">
            <a href="/galleries/new" class="waves-effect waves-light btn amber darken-2"><i class="material-icons right">file_upload</i>NEW VISIT</a>
        </div>
        <div class="divider"></div><br>
        <div class="row">
            {{if .}}
                {{range .}}
                    <div class="col s12 m6 l4">
                        <div class="card z-depth-2">
                            <div class="card-content indigo lighten-2">
                                <div class="card-title">
                                    <span>
                                        <h5 class="white-text"><b>Visit:</b> #{{.ID}}</h5>
                                    </span>
                                </div>
                            </div>
                            <div class="card-content">
                                <p><i class="material-icons left indigo-text text-lighten-2">accessibility</i>Reason:</p><br>
                                {{if .Reason}}
                                    <p><b>{{.Reason}}</b></p>
                                {{else}}
                                    <p class="text-lighten-3">(No Reason Given)</p>
                                {{end}}<br>
                                <div class="divider"></div><br>
                                <p><i class="material-icons left indigo-text text-lighten-2">date_range</i>Date:</p><br>
                                    <div class="center"><b>{{.CreatedAt}}</b></div><br>
                                <div class="divider"></div><br>
                                <p><i class="material-icons left indigo-text text-lighten-2">attach_money</i>Cost:</p><br>
                                {{if .Cost}}
                                    <p class="center"><b>${{.Cost}}</b></p>
                                {{else}}
                                    <p class="text-lighten-3">(No Cost Given)</p>
                                {{end}}
                                <br>
                                <div class="divider"></div><br>
                                <p><i class="material-icons left indigo-text text-lighten-2">receipt</i>Bill Uploaded:</p><br>
                                {{if .Images}}
                                    <p class="center"><b>YES</b></p>
                                {{else}}
                                    <p class="text-lighten-3 center"><b>No</b></p>
                                {{end}}
                            </div>
                            <div class="card-action center">
                                <a href="/galleries/{{.ID}}" class="btn indigo lighten-2"><i class="material-icons right">keyboard_arrow_right</i>View</a>
                            </div>
                        </div>
                    </div>
                {{end}}
            {{else}}
                <div class="col s12 m8 l6 offset-m2 offset-l3">
                    <div class="center">   
                        <h4 class="indigo-text text-lighten-3">No Visits yet<h4><br>
                        <h5 class="indigo-text text-lighten-3">You must be immortal</h5><br>
                    </div>
                </div>
            {{end}}
        </div>
        <!--<div class="row">-->
        <!--    <div class="col s12 m6">-->
        <!--        <ul>-->
        <!--            <li>*Consider moving to seperate site menu item</li>-->
        <!--            <li>Estimated contributions this month</li>-->
        <!--            <li>Pool size</li>-->
        <!--            <li>Number of contributors</li>-->
        <!--            <li>Average medical expenses per customers</li>-->
        <!--        </ul>-->
        <!--    </div>-->
        <!--</div>-->
    </div>
{{end}}