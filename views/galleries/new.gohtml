{{define "yield"}}
    <div class="container">
        <div class="row center">
            <br>
            <img src="data:image/svg+xml;utf8;base64," />
            <h3 class="indigo-text text-lighten-2">VISIT</h3>  
        </div>
        {{template "visitInformation"}}
        {{template "visitForm"}}
    </div>
{{end}}

{{define "visitInformation"}}
    <div class="row">
        <div class="col s12 m8 offset-m2">
            <blockquote>
                <h4>INFO</h4>
                <p>Please fill out the form below to begin processing your recent visit. <br><br>
                You will be asked to include an image of your bill in the next step. Please have this ready.</p>
            </blockquote>
        </div>
    </div>
{{end}}

{{define "visitForm"}}
    <div class="row">
        <form action="/galleries" method="POST">
            {{csrfField}}
            <div class="row">
                <div class="col s12 m8 offset-m2">
                    <div class="card hoverable">
                        <div class="card-content">
                            <div class="card-title">
                                <h6>What is the reason for your visit?</h6>
                            </div>
                            <div class="divider"></div><br>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <i class="material-icons prefix indigo-text text-lighten-1">accessibility</i>
                                    <input id="gallery-title" type="text" class="validate" name="reason" data-length="48">
                                    <label for="gallery-title" data-error="Invalid" data-success="Accepted">Reason</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m8 offset-m2">
                    <div class="card hoverable">
                        <div class="card-content">
                            <div class="card-title">
                                <h6>What is your visit's total bill cost?</h6>
                            </div>
                            <div class="divider"></div><br>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <i class="material-icons prefix indigo-text text-lighten-1">attach_money</i>
                                    <input id="gallery-cost" type="text" class="validate" name="cost" data-length="48">
                                    <label for="gallery-cost" data-error="Invalid" data-success="Accepted">Cost</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m8 offset-m2">
                    <div class="card hoverable">
                        <div class="card-content">
                            <div class="card-title">
                                <h6>Who is the doctor providing medical services?</h6>
                            </div>
                            <div class="divider"></div><br>
                            <div class="row">
                                <div class="input-field col s11 m8">
                                    <i class="material-icons prefix indigo-text text-lighten-1">assignment_ind</i>
                                    <input id="gallery-title" type="text" class="validate" name="doctor" data-length="48">
                                    <label for="gallery-title" data-error="Invalid" data-success="Accepted">Doctor</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col s12 m8 offset-m2">
                    <div class="card hoverable">
                        <div class="card-content">
                            <div class="card-title">
                                <h6>Medical office information</h6>
                            </div>
                            <div class="divider"></div><br>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <i class="material-icons prefix indigo-text text-lighten-1">location_city</i>
                                    <input id="gallery-title" type="text" class="validate" name="address" data-length="99">
                                    <label for="gallery-title" data-error="Invalid" data-success="Accepted">Address</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <input id="gallery-title" type="text" class="validate" name="city" pattern="[A-Za-z]{,66}" data-length="66">
                                    <label for="gallery-title" data-error="Invalid" data-success="Accepted">City</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <input id="gallery-title" type="text" class="validate" name="state" pattern="[A-Za-z]{,52}" data-length="52">
                                    <label for="gallery-title" data-error="Invalid" data-success="Accepted">State</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <input id="gallery-title" type="text" class="validate" name="zipcode" pattern="[0-9]{5}" data-length="5">
                                    <label for="gallery-title" data-error="Invalid. 5 Digit Zip Codes Please" data-success="Accepted">Zip Code (5 Digit)</label>
                                </div>
                            </div>
                            <div class="row">
                                <div class="input-field col s11 m10">
                                    <i class="material-icons prefix indigo-text text-lighten-1">phone</i>
                                    <input id="gallery-title" type="tel" class="validate" name="phonenumber" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}" data-length="12">
                                    <label for="gallery-title" data-error="Invalid. (i.e 888-888-8888)" data-success="Accepted">Phone Number</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col s12 m12 center">
                <button type="submit" class="waves-effect btn btn-large indigo lighten-2 z-depth-2">
                   <i class="material-icons">keyboard_arrow_right</i> 
                </button>
            </div>
        </form>
    </div>
{{end}}
