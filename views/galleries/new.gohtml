{{define "yield"}}
    <div class="container">
        <div class="row center">
            <br>
            <img src="data:image/svg+xml;utf8;base64," />
            <h4 class="blue-grey-text text-lighten-1">GALLERY</h4>  
        </div>
        <div class="row center">
            <h5>New Contribution</h5>
        </div>
        <div class="row">
            {{template "galleryForm"}}
        </div>
    </div>
{{end}}

{{define "galleryForm"}}
    <div class="col s12 m8 offset-m2 card hoverable z-depth-1">
        <div class="card-content">
            <form action="/galleries" method="POST">
                {{csrfField}}
                <div class="row">
                    <div class="input-field">
                        <input id="gallery-title" type="text" class="validate" name="title" data-length="48">
                        <label for="gallery-title" data-error="Too long" data-success="Accepted">Title</label>
                    </div>
                </div>
                <br>
                <div class="row">
                    <h5>Images</h5><br>
                    <form action="#">
                        <div class="file-field input-field">
                            <div class="btn">
                                <span>File</span>
                                <input type="file" multiple>
                            </div>
                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text" placeholder="Upload one or more images">
                            </div>
                        </div>
                        <div class="row right-align">
                            <button type="submit" class="btn waves-effect waves-light red lighten-3">
                                <i class="material-icons">file_upload</i>
                            </button>
                        </div>
                    </form>
                </div><br>
                <div class="card-action center"><br>
                    <button type="submit" class="btn btn-large waves-effect waves-light red lighten-3">
                        <i class="material-icons">add</i>
                    </button>
                </div>
            </form>
        </div>
    </div>
{{end}}
