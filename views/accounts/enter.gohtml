{{define "yield"}}
    <div class="container"><br>
        <div class="row">
            {{template "enterForm"}}
        </div>
    </div>
{{end}}

{{define "enterForm"}}
    <form action="/enter" method="POST" class="col s12 m8 l6 offset-m2 offset-l3">
        {{csrfField}}
        <div class="card hoverable z-depth-4">
            <div class="card-content">
                <div class="row center"><br>
                    <img src="data:image/svg+xml;utf8;base64," />
                </div><br>
                <span class="card-title">
                    <blockquote class="indigo-color">
                        <h4 class="condensed light">LOGIN</h4>
                    </blockquote>
                </span><br>
                <div class="row">
                    <div class="input-field col s11 m11">
                        <i class="material-icons prefix indigo-text text-lighten-1">account_circle</i>
                        <input id="email" type="email" class="validate" name="email">
                        <label for="email" data-error="Incorrect Format. Try Again." data-success="Correct Format">EMAIL</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s11 l11">
                        <i class="material-icons prefix indigo-text text-lighten-1">lock_outline</i>
                        <input id="password" type="password" class="validate" name="password">
                        <label for="password">PASSWORD</label>
                    </div>
                </div>
                <div class="center-align"><br>
                    <button type="submit" class="btn waves-effect waves-light indigo lighten-2">
                        <i class="material-icons">keyboard_arrow_right</i>
                    </button>
                </div><br><br>
                <div class="card-divider">
                    <div class="card-action"><br>
                        <a  href="/register" class="waves-effect waves-light indigo-text text-lighten-2 left-align">
                           <i class="material-icons">person_add</i>
                        </a>
                        <a class="waves-effect waves-light indigo-text text-lighten-2 right">
                           <i class="material-icons">help_outline</i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </form>
{{end}}